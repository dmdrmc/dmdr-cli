@font-face {
	font-family: "{{fontName}}";
	src: {{{strip_hashtag src}}};
}

.{{baseSelector}} {
	line-height: 1;
}

.{{baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: top;
}

{{#each codepoints}}
.{{../baseSelector}}.{{../classPrefix}}{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}